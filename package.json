{
  "name": "ayesaac_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "run-p lint:**",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:types": "tsc --noEmit",
    "fix": "run-p fix:**",
    "fix:es": "eslint --ext .js,.jsx,.ts,.tsx  --fix ./",
    "generate": "plop --plopfile ./scripts/generators/plopfile.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --noEmit -p tsconfig.json"
    }
  },
  "lint-staged": {
    "*.{js?(x),ts?(x)}": "eslint --fix",
    "*.{html,md,json,yml,yaml,css}": "prettier --write"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@reduxjs/toolkit": "^1.3.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.3",
    "react-is": "^16.13.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "theme-ui": "^0.3.1",
    "typescript": "~3.8.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^25.1.1",
    "@types/node": "^12.12.26",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "^5.0.1",
    "@types/theme-ui": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-unicorn": "^18.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.10",
    "npm-run-all": "^4.1.5",
    "plop": "^2.6.0",
    "prettier": "^2.0.0"
  }
}
